#include<stdio.h>
#include<string.h>
char input[100]; int i=0;
int E(), Ed(), T(), Td(), F();
void main() {
    printf("Enter string: "); scanf("%s", input);
    if(E() && input[i]=='\0') printf("Success\n");
    else printf("Error\n");
}
int E() { if(T()) { if(Ed()) return 1; } return 0; }
int Ed() { if(input[i]=='+') { i++; if(T()) return Ed(); return 0; } return 1; }
int T() { if(F()) { if(Td()) return 1; } return 0; }
int Td() { if(input[i]=='*') { i++; if(F()) return Td(); return 0; } return 1; }
int F() { if(input[i]=='(') { i++; if(E()) { if(input[i]==')') { i++; return 1; } } }
    else if(isalpha(input[i])) { i++; return 1; } return 0; }
